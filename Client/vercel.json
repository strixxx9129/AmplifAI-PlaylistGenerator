{
    "framework": "create-react-app",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://amplifai-server.vercel.app/api/$1"
        },
        {
            "source": "/album",
            "destination": "https://amplifai-server.vercel.app/album"
        },
        {
            "source": "/getEmotionPlaylist",
            "destination": "https://amplifai-server.vercel.app/getEmotionPlaylist"
        },
        {
            "source": "/addToLikedSongs",
            "destination": "https://amplifai-server.vercel.app/addToLikedSongs"
        },
        {
            "source": "/likedSongs",
            "destination": "https://amplifai-server.vercel.app/likedSongs"
        },
        {
            "source": "/findSongs",
            "destination": "https://amplifai-server.vercel.app/findSongs"
        },
        {
            "source": "/getPlaylist",
            "destination": "https://amplifai-server.vercel.app/getPlaylist"
        },
        {
            "source": "/getPlaylist/(.*)",
            "destination": "https://amplifai-server.vercel.app/getPlaylist/$1"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin-allow-popups"
                }
            ]
        }
    ]
}